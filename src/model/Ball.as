package model {		public class Ball {		public var x: Number;		public var y: Number;		public var xVelocity: Number = -2 + Math.random() * 4;		public var yVelocity: Number = -2 + Math.random() * 4;		public function Ball(x: Number, y: Number) {			this.x = x;			this.y = y;		}		public function setVelocity(xVelocity: Number, yVelocity: Number): void {			this.xVelocity = xVelocity;			this.yVelocity = yVelocity;		}				public function update(): void {			this.x = this.x + this.xVelocity;			this.y = this.y + this.yVelocity;		}	}}